<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>Laravel Boot</title>
  <parser>
    <target>/tmp</target>
  </parser>
  <transformer>
    <target>docs/phpdoc</target>
  </transformer>
  <transformations>
    <template name="clean" />
  </transformations>
  <files>
    <directory>app</directory>
    <directory>bootstrap</directory>
    <ignore>bootstrap/cache/*</ignore>
    <directory>config</directory>
    <directory>database</directory>
    <directory>routes</directory>
    <directory>tests</directory>
  </files>
</phpdoc>
